{
    "name": "peynman/FaraPayamak",
    "description": "FaraPayamak sms service provider for laravel.",
    "type": "library",
    "version": "1.2-dev",
    "require": {
        "laravel/framework": "5.*"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^2.4",
        "orchestra/testbench": "^3.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Peyman Abdi"
        }
    ],
    "autoload": {
        "psr-4": {
            "FaraPayamak\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FaraPayamak\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FaraPayamak\\Providers\\FaraPayamakServiceProvider"
            ],
            "aliases": {
            },
            "dont-discover": [
            ]
        }
    }
}
